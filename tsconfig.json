{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "module": "es6",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "outDir": "./tsc/",
    "strictNullChecks": true,
    "target": "ES2019",
    "jsx": "react",
  }
}
